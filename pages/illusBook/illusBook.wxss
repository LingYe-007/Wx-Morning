.nav-content{
  background:white;
}
.catList{
  padding: 0 50rpx;
  overflow-y: auto;
}
.container{
  padding-bottom: 40rpx;
  background:#E6E6E6;
}
.main{
  background:#E6E6E6;
}
.catList{
  overflow-y: auto;
  padding-top: 100rpx;
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 40rpx;
}
.catList>view{
  background: white;
  height: calc((100vw - 140rpx) / 2 * 16 / 13);
  border: 1rpx solid #707070;
  border-radius: 16rpx;
}
.catList>view>image{
  width: 100%;
  height:calc((100vw - 140rpx)/2 /13 * 11) ;
}
.catList>view>ul{
  display: block;
  font-size: 32rpx;
  position: relative;
  padding-left: 14rpx;
}
.catList>view>ul::before{
  content: "";
  position: absolute;
  top: 50rpx;
  background:rgba(250, 185, 35, 1);
  border-radius: 5rpx;
  width: 45rpx;
  height: 10rpx;
}
.catList>view>span{
  margin: 15rpx 0;
  display: block;
  font-size: 24rpx;
  color: #707070;
  padding-left: 16rpx;
}
.search{
  width: 100vw;
  padding-top: 30rpx;
  padding-bottom: 40rpx;
  position: fixed;
  background:#E6E6E6;
  z-index: 10;
}
.ul{
  display: flex; 
  align-items: center;
  margin-top: 10rpx; 
  margin-left: 14rpx;
}
.ul>image{
  position: relative;
  /* top: 5rpx; */
  margin-left: 16rpx;
  /* margin-top: 10rpx; */
  width: 30rpx;
  height: 30rpx;
  z-index: 3;
}